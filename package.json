{
  "name": "auth0-nextjs-sample",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": "~16.15.1"
  },
  "scripts": {
    "dev": "concurrently \"next dev\" \"node api-server\"",
    "dev:api": "nodemon api-server",
    "build": "next build",
    "start": "concurrently \"next start\" \"node api-server\"",
    "start:api": "node api-server",
    "test": "jest tests --coverage --maxWorkers=10",
    "test:watch": "jest --coverage --watch",
    "test:integration": "start-server-and-test start http-get://localhost:3000 cypress:run",
    "test:integration:watch": "start-server-and-test start http-get://localhost:3000 cypress:open",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^1.9.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@heroicons/react": "^1.0.6",
    "@stripe/stripe-js": "^1.32.0",
    "autoprefixer": "^10.4.7",
    "axios": "^0.27.2",
    "canvas-confetti": "^1.5.1",
    "concurrently": "^7.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-jwt": "^7.7.5",
    "helmet": "^5.1.0",
    "highlight.js": "^11.5.1",
    "jwks-rsa": "^2.1.4",
    "micro": "^9.3.4",
    "morgan": "^1.10.0",
    "next": "^12.2.0",
    "node": "^18.4.0",
    "nodemon": "^2.0.18",
    "postcss": "^8.4.14",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.2.0",
    "reactstrap": "^9.1.1",
    "stripe": "^9.11.0",
    "swr": "^1.3.0",
    "tailwindcss": "^3.1.4"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.14.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "babel-jest": "^28.1.2",
    "cypress": "^10.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.2",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "rootDir": "./",
    "moduleNameMapper": {
      ".(css)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "setupFiles": [
      "./tests/environment.js"
    ],
    "setupFilesAfterEnv": [
      "./tests/setup.js"
    ],
    "collectCoverageFrom": [
      "components/**/*.*",
      "pages/**/*.*"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary"
    ]
  }
}
